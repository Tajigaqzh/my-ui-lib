{
  "name": "my-ui-lib-monorepo",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "setup": "pnpm install && pnpm -r run build",
    "dev:core": "pnpm --filter @my-ui/core dev",
    "dev:docs": "pnpm --filter @my-ui/docs dev",
    "build:core": "pnpm --filter @my-ui/core run build",
    "build:utils": "pnpm --filter @my-ui/utils run build",
    "build:packages": "pnpm -r --filter './packages/*' run build",
    "build:all": "pnpm -r run build",
    "test:all": "pnpm -r run test",
    "lint:all": "pnpm -r run lint",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "dev": "pnpm -r run dev",
    "test": "pnpm -r run test",
    "lint": "pnpm -r run lint",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish",
    "ci:version": "changeset version --snapshot dev",
    "ci:publish": "changeset publish --tag dev",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@types/node": "^20.19.27",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/test-utils": "^2.0.0",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "sass": "^1.97.1",
    "typescript": "^5.9.3",
    "vite": "5.4.21",
    "vitepress": "2.0.0-alpha.15",
    "vitest": "^4.0.16",
    "vue-tsc": "^1.8.27",
    "vite-plugin-dts": "^4.5.4"
  },
  "changelog": [
    "./scripts/changelog-generator.js",
    null
  ]
}
