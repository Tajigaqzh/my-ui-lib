{
  "name": "my-ui-lib-monorepo",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@10.18.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "setup": "pnpm install && pnpm run build",
    "build": "vite build",
    "build:all": "turbo run build",
    "dev:playground": "pnpm --filter playground dev",
    "build:playground": "pnpm --filter playground build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"**/*.{js,ts,vue,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,vue,json,md}\"",
    "commit": "git add . && git-cz",
    "clean": "rimraf dist",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish",
    "ci:version": "changeset version --snapshot dev",
    "ci:publish": "changeset publish --tag dev",
    "docs:dev": "vitepress dev docs --port=5174",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@commitlint/cz-commitlint": "^20.2.0",
    "@types/node": "^20.19.27",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "4.0.16",
    "@vitest/ui": "^4.0.16",
    "@vue/language-core": "^3.2.1",
    "@vue/runtime-core": "^3.5.26",
    "@vue/test-utils": "^2.0.0",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "fast-check": "^4.5.2",
    "husky": "^9.1.7",
    "inquirer": "^8.2.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "sass": "^1.97.1",
    "turbo": "^2.7.2",
    "typescript": "^5.9.3",
    "vite": "5.4.21",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "2.0.0-alpha.15",
    "vitest": "^4.0.16",
    "vue": "^3.5.26",
    "vue-tsc": "^1.8.27"
  },
  "changelog": [
    "./scripts/changelog-generator.js",
    null
  ],
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-git"
    }
  }
}
