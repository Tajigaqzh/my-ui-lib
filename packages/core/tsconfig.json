{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "@/*": ["src/*"],
      "@my-ui/utils": ["../utils/src"],
      "@my-ui/theme": ["../theme/src"],
      "@my-ui/icons": ["../icons/src"]
    }
  },
  "vueCompilerOptions": {
    "target": 3
  },
  "include": ["**/*.ts", "**/*.vue", "**/*.tsx", "shims-vue.d.ts"],
  "exclude": ["node_modules", "dist"]
}
